#ifndef _MegaVTK2_Configure_h_
#define _MegaVTK2_Configure_h_

/* Whether we are building shared libraries.  */
#cmakedefine VTK_BUILD_SHARED_LIBS

#if defined (WIN32) && defined (VTK_BUILD_SHARED_LIBS)
 #if defined (QMEGAVTKADDON2_EXPORTS)
  #define QMEGAVTKADDON2_EXPORT __declspec( dllexport )
 #else
  #define QMEGAVTKADDON2_EXPORT __declspec( dllimport )
 #endif
 #if defined (vtkRenderingAddOn2_EXPORTS)
  #define VTK_RENDERINGADDON2_EXPORT __declspec( dllexport )
 #else
  #define VTK_RENDERINGADDON2_EXPORT __declspec( dllimport )
 #endif

#else

  #define QMEGAVTKADDON2_EXPORT
  #define VTK_RENDERINGADDON2_EXPORT

#endif

#endif
